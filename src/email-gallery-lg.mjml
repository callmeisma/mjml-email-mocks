    <!-- FOOTER -->
    <mj-section
      background-color="white"
      background-url=""
      background-position="54% 97.5%"
      padding="3% 0"
    >
      <mj-column vertical-align="middle">
        <mj-image
          src="https://233d8d9d1be77a42cc4e-cb4d167347d4238c9e99ff4a171716c6.ssl.cf1.rackcdn.com/2022/VFF-LOGO_Black.png"
